+		btn.text = "%s (%d MP)" % [skill["name"], skill["mp_cost"]]
+		btn.size_flags_horizontal = Control.SIZE_EXPAND_FILL
+		btn.pressed.connect(_on_skill_selected.bind(skill))
+		skill_list_container.add_child(btn)
